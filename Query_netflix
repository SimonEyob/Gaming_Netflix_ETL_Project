Drop table netflix_movies;
Drop table netflix_tvshows;

Create Table netflix_movies(
show_id varchar,
type varchar,
title varchar,
date_added varchar,
rating varchar,
duration varchar,
listed_in varchar,
primary key (show_id)
);

Create Table netflix_tvshows(
show_id Varchar,
type varchar,
title varchar,
date_added varchar,
rating varchar,
duration varchar,
listed_in varchar,
primary key (show_id)
);